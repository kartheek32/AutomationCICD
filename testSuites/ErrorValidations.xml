<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel="tests" name="Suite">


	<groups>
		<run>
			<include name="ErrorHandling"/>
		</run>
	</groups>
	
	<listeners>
<listener class-name= "FrameworkBuild.TestComponents.Listeners"/>
</listeners>
	<test thread-count="5" name="Submit order test">
		<classes>
			<class name="FrameworkBuild.Tests.StandAlone" />
		</classes>
	</test> <!--
	Test -->
	<test thread-count="5" name="Error validation test">
		<classes>
			<class name="FrameworkBuild.Tests.ErrorValidations" />
		</classes>
	</test> <!--
	Test -->
</suite> <!--
Suite -->
